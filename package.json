{
  "name": "mongoscraper_v2",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "dependencies": {
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "mongoose": "^5.3.5",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-materialize": "^2.4.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.0.5"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && yarn install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "description": "A MERN application where you can get updated on all your news from New York Times!",
  "main": "server.js",
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/btburns10/MongoScraper-v2.git"
  },
  "author": "Brendan Burns",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/btburns10/MongoScraper-v2/issues"
  },
  "homepage": "https://github.com/btburns10/MongoScraper-v2#readme"
}
