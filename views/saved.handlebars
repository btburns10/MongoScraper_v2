  <div class="wrapper">

    <div id="article">
      <div class="row">
        <div class="col s12 m9 lg9">
          {{#each savedArticles}}
            <div class="card #546e7a blue-grey darken-1">
              <div class="card-content white-text" data-id="{{this._id}}">
                <span class="card-title">{{this.title}}</span>
                <hr>
                <p class="summary">{{this.summary}}</p>
              </div>
              <div class="card-action #cfd8dc blue-grey lighten-4">
                <a href="{{this.link}}" target="_blank" style="color: #0091ea">Click here to read more!</a>
                <button id="deleteBtn" data-id="{{this._id}}" class="waves-effect #e57373 red lighten-2 btn">Remove</button>
                <button id="noteBtn" data-id="{{this._id}}" class="waves-effect btn">Make Note</button>
              </div>
            </div>
          {{/each}}
        </div>
        <div class="col s12 m3 lg3">
          <h4>Notes</h4>
          <div id="notes"></div>
        </div>
      </div>
    </div>

  </div>